<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="301.0" prefWidth="413.0" stylesheets="@../iugventanas.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mvc.vista.iugraficaventanas.controladoresvistas.ControladorMostrarVehiculo">
   <children>
      <GridPane hgap="10.0" vgap="10.0">
         <children>
            <Label text="Tipo Vehiculo:" GridPane.halignment="RIGHT" />
            <Label text="Matricula:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <Label text="Marca:" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
            <Label text="Modelo:" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
            <Label text="Numero Plazas:" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
            <Label text="Cilindrada:" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
            <TextField fx:id="tfMatricula" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="tfMarca" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="tfModelo" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="tfNumeroPlazas" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="tfCilindrada" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="tfTipo" disable="true" GridPane.columnIndex="1" />
            <Label text="PMA:" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
            <TextField fx:id="tfPma" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="6" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="104.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="217.0" minWidth="10.0" prefWidth="217.0" />
         </columnConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <HBox alignment="CENTER" prefHeight="54.0" prefWidth="358.0" spacing="20.0">
         <children>
            <Button fx:id="btBorrar" mnemonicParsing="false" onAction="#borrarVehiculo" text="Borrar" />
            <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
