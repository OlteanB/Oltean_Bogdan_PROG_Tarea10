<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="184.0" prefWidth="386.0" stylesheets="@../iugventanas.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mvc.vista.iugraficaventanas.controladoresvistas.ControladorAnadirAlquiler">
   <children>
      <GridPane hgap="10.0" prefHeight="130.0" prefWidth="485.0" vgap="10.0">
         <children>
            <Label text="DNI Cliente:" GridPane.halignment="RIGHT" />
            <Label text="Matricula:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <TextField fx:id="tfDni" disable="true" editable="false" GridPane.columnIndex="1" />
            <TextField fx:id="tfMatricula" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="189.0" minWidth="10.0" prefWidth="123.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="267.0" minWidth="10.0" prefWidth="233.0" />
         </columnConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <HBox alignment="CENTER" prefHeight="18.0" prefWidth="366.0" spacing="20.0">
         <children>
            <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
            <Button fx:id="btAnadir" mnemonicParsing="false" onAction="#crearAlquiler" text="Aceptar" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
